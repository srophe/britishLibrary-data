                  <msItem xml:id="d11" n="15">
                    <locus from="101b" to="103">Foll. 101b-103</locus>
                    <title xml:lang="en">The first.</title>
                  </msItem>
                  <msItem xml:id="d12" n="16">
                    <locus from="104a" to="108">Foll. 104a-108</locus>
                    <title xml:lang="en">Second.</title>
                  </msItem>
                  <msItem xml:id="d13" n="17">
                    <locus from="109a" to="131">Foll. 109a-131</locus>
                    <title xml:lang="en">Third.</title>
                  </msItem>
                  <msItem xml:id="d14" n="18">
                    <locus from="132a" to="153">Foll. 132a-153</locus>
                    <title xml:lang="en">Fourth.</title>
                  </msItem>
                  <msItem xml:id="d15" n="19">
                    <locus from="154a"/>
                    <title xml:lang="en">Fifth.</title>
                  </msItem>
                  <msItem xml:id="d16" n="20">
                    <locus from="154a"/>
                    <title xml:lang="en">sixth hymn.</title>
                    <note>the scribe has omitted to mark the commencement of the sixth hymn</note>
