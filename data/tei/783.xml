            <handDesc hands="2">
              <handNote xml:id="handNote1" scope="major" script="syr-Syre" medium="unknown">
                This volume is written in a good, regular Esṭrangělā of the <date notBefore="0600" notAfter="0700" calendar="Gregorian">viith cent</date>.
              </handNote>
