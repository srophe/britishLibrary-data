            <handDesc hands="3">
              <handNote xml:id="handNote1" scope="major" script="syr-Syre" medium="unknown">
                This manuscript is written in a fine, regular Estrangělā, and dated <date datingMethod="Seleucid" when-custom="0880">A. Gr. 880</date>, corresponding to the year 617 of the era of Antioch, or <date calendar="Gregorian" when="0569">A.D. 569</date>.
              </handNote>
              <handNote xml:id="handNote2" scope="minor" script="syr" medium="unknown">
                <ref target="addition2">See additions</ref>
                </handNote>
