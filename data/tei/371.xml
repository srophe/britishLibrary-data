              <handDesc hands="2">
                <handNote xml:id="p1handNote1" medium="unknown" scope="major" script="syr">
                  The writing is neat and regular, of the <date>viiith or ixth cent.</date>
                  </handNote>
                <handNote xml:id="p1handNote2" medium="unknown" scope="minor" script="syr">
                  See additions.
                </handNote>
