                                        <dimensions type="leaf" unit="inch">
                      <height quantity="7.25">7 1/4</height>
                      <width quantity="5.25">5 1/4</width>
                    </dimensions>
                                    </extent>
                                    <collation>
                                        <p>The quires are without signatures.</p>
                                    </collation>
