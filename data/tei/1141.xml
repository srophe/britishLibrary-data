            <handDesc hands="2">
              <handNote xml:id="handNote1" medium="unknown" scope="sole" script="syr">
                This volume is written in a good, regular hand, with many Greek and Syriac vowels.</handNote>
              <handNote xml:id="handNote2" medium="unknown" scope="sole" script="syr">
